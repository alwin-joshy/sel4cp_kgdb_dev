<?xml version="1.0" encoding="UTF-8"?>
<system>

	<memory_region name="uart" size="0x1000" phys_addr="0xc8100000" />

	<protection_domain name="client">
		<program_image path="client.elf" />
		<map mr="uart" vaddr="0x4000000" perms="rw" cached="false" setvar_vaddr="uart_base_vaddr" />
	</protection_domain>

	<protection_domain name="server" pp="true">
		<program_image path="server.elf" />
		<!-- <map mr="uart" vaddr="0x4000000" perms="rw" cached="false" setvar_vaddr="uart_base_vaddr" /> -->
	</protection_domain>

	<channel>
		<end pd="client" id="0" />
		<end pd="server" id="0" />
	</channel>
</system>
